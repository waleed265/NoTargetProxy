                  "path":"/applications/*",
                  "permissions":[
                     "get",
                     "put"
                  ]
               },
               {
                  "path":"/environments/*/application/*/revisions/*/debugsessions",
                  "permissions":[
                     "get",
                     "put"
                  ]
               },
               {
                  "path":"/keyvaluemaps/*",
