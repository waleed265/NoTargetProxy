{
   "version":"1.0",
   "envConfig":{
      "test":{
         "targetServers":[
            {
               "name":"HTTPBin",
               "host":"httpbin.org",
               "isEnabled":true,
               "port":80
            }
         ],
         "virtualHosts":[
            {
               "name":"example",
               "hostAliases":[
                  "example.com"
               ],
               "isEnabled":true,
               "port":80
            }
         ],
         "references":[
            {
               "name":"sampleReference",
               "refers":"freetrial",
               "resourceType":"KeyStore"
            }
         ],
         "caches":[
            {
               "name":"echocache",
               "description":"echo cache",
               "expirySettings":{
                  "timeoutInSec":{
                     "value":"300"
                  },
                  "valuesNull":false
               }
            }
         ],
         "resourcefiles":[
            {
               "name":"test",
               "type":"jsc",
               "file":"./resourceFiles/jsc/test.js"
            }
         ],
         "flowhooks":[
            {
               "name":"PreProxyFlowHook",
               "continueOnError":"true",
               "sharedFlow":"corspreflow"
            },
            {
               "name":"PreTargetFlowHook",
               "continueOnError":"true",
               "sharedFlow":"corspreflow"
            }
         ],
         "extensions":[
            {
               "name":"test-extension",
               "description":"Testing",
               "packageName":"gcp-stackdriver-logging",
               "version":"1.2.1"
            }
         ],
         "keystores":[
            {
               "name":"testKeyStorename"
            }
         ],
         "aliases":[
            {
               "alias":"testSelfSignedCert",
               "keystorename":"testKeyStorename",
               "format":"selfsignedcert",
               "keySize":"2048",
               "sigAlg":"SHA256withRSA",
               "subject":{
                  "commonName":"testcommonName"
               },
               "certValidityInDays":"60"
            },
            {
               "alias":"testAliasKeyCertFileAndKey",
               "keystorename":"testKeyStorename",
               "format":"keycertfile",
               "certFilePath":"/tmp/certs/keystore.pem",
               "keyFilePath":"/tmp/certs/keystore.key",
               "password":"dummy"
            },
            {
               "alias":"testAliasKeyCertJar",
               "keystorename":"testKeyStorename",
               "format":"keycertjar",
               "filePath":"/tmp/certs/myKeystore.jar",
               "password":"dummy"
            },
            {
               "alias":"testAliasPKCS12",
               "keystorename":"testKeyStorename",
               "format":"pkcs12",
               "filePath":"/tmp/certs/myKeystore.p12",
               "password":"dummy"
            }
         ]
      }
   },
   "orgConfig":{
      "specs":[
         {
            "name":"Petstore-v2",
            "file":"./specs/petstore-v2.yaml"
         }
      ],
      "apiProducts":[
